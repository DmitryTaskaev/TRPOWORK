<Window x:Class="practica_7.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:practica_7"
        xmlns:converters="clr-namespace:practica_7.Converters"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
    <Grid>
        <StackPanel>
            <WrapPanel>
                <Button Content="ListBox" Width="260" Height="50" Name="List" Click="List_Click"/>
                <Button Content="DataGrid" Width="260" Name="Grid" Click="Grid_Click"/>
                <Button Content="WrapPanel" Width="260" Name="Wrap" Click="Wrap_Click"/>
            </WrapPanel>
            <Frame Height="367" Name="frame"/>
        </StackPanel>
    </Grid>
</Window>
